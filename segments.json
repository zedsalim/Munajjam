[
  {
    "id": 1,
    "start": 0.31,
    "end": 4.37,
    "text": "بِسْمِ اللَّهِ الرَّحْمَنِ الرَّحِيمِ"
  },
  {
    "id": 2,
    "start": 5.38,
    "end": 8.53,
    "text": "يَا أَيُّهَا الْمُدَّثِّينَ"
  },
  {
    "id": 3,
    "start": 9.46,
    "end": 11.07,
    "text": "قُمْ فَأَنْذِرْ"
  },
  {
    "id": 4,
    "start": 11.94,
    "end": 13.71,
    "text": "وَرَبَّكَ سَكَبِّرَ"
  },
  {
    "id": 5,
    "start": 14.56,
    "end": 16.56,
    "text": "وَثِيَابَكَ فَطَهِّي"
  },
  {
    "id": 6,
    "start": 17.51,
    "end": 19.13,
    "text": "وَالرُّجْزَ فَاهْجُرْ"
  },
  {
    "id": 7,
    "start": 20.08,
    "end": 22.51,
    "text": "وَلَا تَمْنُنْ تَسْتَ"
  },
  {
    "id": 8,
    "start": 22.82,
    "end": 22.96,
    "text": "وَالْمُؤْمِنِينَ"
  },
  {
    "id": 9,
    "start": 23.85,
    "end": 25.62,
    "text": "وَلِرَبِّكَ فَاصْبِرْ"
  },
  {
    "id": 10,
    "start": 26.63,
    "end": 30.43,
    "text": "فَإِذَا نُقِرَ فِي النَّاقُورِ"
  },
  {
    "id": 11,
    "start": 31.22,
    "end": 35.72,
    "text": "فَذَلِكَ يَوْمَئِذٍ يَوْمٌ عَسِيرٌ"
  },
  {
    "id": 12,
    "start": 36.54,
    "end": 40.36,
    "text": "عَلَى الْكَافِرِينَ غَيْرُ يَسِيرٍ"
  },
  {
    "id": 13,
    "start": 41.41,
    "end": 44.67,
    "text": "ذَرْنِي وَمَنْ خَلَقْتُ وَحِيدًا"
  },
  {
    "id": 14,
    "start": 45.44,
    "end": 49.16,
    "text": "وَجَعَلْتُ لَهُ مَالًا مَمْدُودًا"
  },
  {
    "id": 15,
    "start": 49.97,
    "end": 52.0,
    "text": "وَبَنِينَ شُهُودًا"
  },
  {
    "id": 16,
    "start": 52.74,
    "end": 53.41,
    "text": "وَمَهَّدَ"
  },
  {
    "id": 17,
    "start": 53.73,
    "end": 55.99,
    "text": "عُيُ لَهُ تَمْهِيدًا"
  },
  {
    "id": 18,
    "start": 56.79,
    "end": 60.23,
    "text": "ثُمَّ يَطْمَعُ أَنْ أَزِيدَ"
  },
  {
    "id": 19,
    "start": 61.01,
    "end": 61.73,
    "text": "كَلَّا"
  },
  {
    "id": 20,
    "start": 62.57,
    "end": 67.02,
    "text": "إِنَّهُ كَانَ لِآيَاتِنَا عَنِيدًا"
  },
  {
    "id": 21,
    "start": 68.04,
    "end": 70.39,
    "text": "سَأُرْهِقُهُ صَعُودًا"
  },
  {
    "id": 22,
    "start": 71.2,
    "end": 74.13,
    "text": "إِنَّهُ فَكَّرَ وَقَدَّرَ"
  },
  {
    "id": 23,
    "start": 74.9,
    "end": 76.99,
    "text": "فَقُتِلَ كَيْفَ قَدَّرَ"
  },
  {
    "id": 24,
    "start": 77.84,
    "end": 80.79,
    "text": "ثُمَّ قُتِلَ كَيْفَ قَدَّرَ"
  },
  {
    "id": 25,
    "start": 81.68,
    "end": 83.14,
    "text": "ثُمَّ نَظَرَ"
  },
  {
    "id": 26,
    "start": 83.9,
    "end": 86.27,
    "text": "ثُمَّ عَبَسَ وَبَسَ"
  },
  {
    "id": 27,
    "start": 87.11,
    "end": 89.82,
    "text": "ثُمَّ أَدْبَرَ وَاسْتَكْبَرَ"
  },
  {
    "id": 28,
    "start": 90.86,
    "end": 96.47,
    "text": "فَقَالَ إِنْ هَذَا إِلَّا سِحْرٌ يُؤْثَرُ"
  },
  {
    "id": 29,
    "start": 97.36,
    "end": 101.19,
    "text": "إِنْ هَذَا إِلَّا قَوْلُ الْبَشَرِ"
  },
  {
    "id": 30,
    "start": 101.95,
    "end": 103.59,
    "text": "سَأُصْلِيهِ سَقَرَ"
  },
  {
    "id": 31,
    "start": 104.54,
    "end": 107.54,
    "text": "وَمَا أَدْرَاكَ مَا سَقَرُ"
  },
  {
    "id": 32,
    "start": 108.32,
    "end": 110.74,
    "text": "لَا تُبْقِي وَلَا تَذَرُ"
  },
  {
    "id": 33,
    "start": 111.43,
    "end": 113.7,
    "text": "لَوَّاحَةٌ لِلْبَشَرِ"
  },
  {
    "id": 34,
    "start": 114.38,
    "end": 116.63,
    "text": "عَلَيْهَا تِسْعَةَ عَشَرَ"
  },
  {
    "id": 35,
    "start": 117.68,
    "end": 124.16,
    "text": "وَمَا جَعَلْنَا أَصْحَابَ النَّارِ إِلَّا مَلَائِكَةً"
  },
  {
    "id": 36,
    "start": 125.19,
    "end": 135.82,
    "text": "وَمَا جَعَلْنَا عِدَّتَهُمْ إِلَّا فِتْنَةً لِلَّذِينَ كَفَرُوا لِيَسْتَيْقِنَا الَّذِينَ أُوتُوا الْكِتَابَ"
  },
  {
    "id": 37,
    "start": 136.94,
    "end": 151.71,
    "text": "لِيَسْتَيْقِنَا الَّذِينَ أُوتُوا الْكِتَابَ وَيَزْدَادَ الَّذِينَ آمَنُوا إِيمَانًا وَلَا يَرْتَابَ الَّذِينَ أُوتُوا الْكِتَابَ وَالْمُؤْمِنُونَ"
  },
  {
    "id": 38,
    "start": 153.1,
    "end": 170.18,
    "text": "وَلَا يَرْتَابَ الَّذِينَ أُوتُوا الْكِتَابَ وَالْمُؤْمِنُونَ وَلِيَقُولَ الَّذِينَ فِي قُلُوبِهِمْ مَرَضٌ وَالْكَافِرُونَ مَاذَا أَرَادَ اللَّهُ بِهَذَا مَثَلًا"
  },
  {
    "id": 39,
    "start": 171.42,
    "end": 179.45,
    "text": "كَذَلِكَ يُضِلُّ اللَّهُ مَنْ يَشَاءُ وَيَهْدِي مَنْ يَشَاءُ"
  },
  {
    "id": 40,
    "start": 180.44,
    "end": 184.61,
    "text": "وَمَا يَعْلَمُ جُنُودَ رَبِّكَ إِلَّا هُوَ"
  },
  {
    "id": 41,
    "start": 185.51,
    "end": 188.85,
    "text": "وَمَا هِيَ إِلَّا ذِكْرَى لِلْبَشَرِ"
  },
  {
    "id": 42,
    "start": 189.83,
    "end": 191.46,
    "text": "إِلَّا وَالْقَمَرِ"
  },
  {
    "id": 43,
    "start": 192.33,
    "end": 194.2,
    "text": "وَاللَّيْلِ إِذْ أَدْبَرَ"
  },
  {
    "id": 44,
    "start": 195.29,
    "end": 198.66,
    "text": "وَالصُّبْحِ إِذَا أَسْفَرَ"
  },
  {
    "id": 45,
    "start": 199.6,
    "end": 202.26,
    "text": "إِنَّهَا لَإِحْدَى الْكُبَرِ"
  },
  {
    "id": 46,
    "start": 203.25,
    "end": 204.98,
    "text": "نَذِيرًا لِلْبَشَرِ"
  },
  {
    "id": 47,
    "start": 205.98,
    "end": 213.04,
    "text": "لِمَنْ شَاءَ مِنْكُمْ أَنْ يَتَقَدَّمَ أَوْ يَتَأَخَّرَ"
  },
  {
    "id": 48,
    "start": 214.31,
    "end": 218.27,
    "text": "كُلُّ نَفْسٍ بِمَا كَسَبَتْ رَهِينَةٌ"
  },
  {
    "id": 49,
    "start": 219.15,
    "end": 222.67,
    "text": "إِلَّا أَصْحَابَ الْيَمِينِ"
  },
  {
    "id": 50,
    "start": 223.57,
    "end": 228.33,
    "text": "فِي جَنَّاتٍ يَتَسَاءَلُونَ"
  },
  {
    "id": 51,
    "start": 229.22,
    "end": 231.35,
    "text": "عَنِ الْمُجْرِمِينَ"
  },
  {
    "id": 52,
    "start": 232.24,
    "end": 234.57,
    "text": "مَا سَلَكَكُمْ فِي سَقَرَ"
  },
  {
    "id": 53,
    "start": 235.61,
    "end": 239.54,
    "text": "قَالُوا لَمْ نَكُ مِنَ الْمُصَلِّينَ"
  },
  {
    "id": 54,
    "start": 240.66,
    "end": 244.07,
    "text": "وَلَمْ نَكُ نُطْعِمُ الْمِسْكِينَ"
  },
  {
    "id": 55,
    "start": 244.94,
    "end": 250.54,
    "text": "وَكُنَّا نَخُوضُ مَعَ الْخَائِضِينَ"
  },
  {
    "id": 56,
    "start": 251.41,
    "end": 255.95,
    "text": "وَكُنَّا نُكَذِّبُ بِيَوْمِ الدِّينِ"
  },
  {
    "id": 57,
    "start": 256.81,
    "end": 260.84,
    "text": "حَتَّى أَتَانَا الْيَقِينُ"
  },
  {
    "id": 58,
    "start": 261.84,
    "end": 267.09,
    "text": "فَمَا تَنْفَعُهُمْ شَفَاعَةُ الشَّافِعِينَ"
  },
  {
    "id": 59,
    "start": 267.83,
    "end": 272.25,
    "text": "فَمَا لَهُمْ عَنِ التَّذْكِرَةِ مُعْرِضِينَ"
  },
  {
    "id": 60,
    "start": 273.09,
    "end": 277.85,
    "text": "كَأَنَّهُمْ حُمُرٌ مُسْتَنْفِرَةٌ"
  },
  {
    "id": 61,
    "start": 278.73,
    "end": 281.24,
    "text": "فَرَّتْ مِنْ قَسْوَرَةٍ"
  },
  {
    "id": 62,
    "start": 282.24,
    "end": 286.48,
    "text": "بَلْ يُرِيدُ كُلُّ امْرِئٍ مِنْهُمْ أَنْ يُرِيدُ"
  },
  {
    "id": 63,
    "start": 286.79,
    "end": 289.48,
    "text": "فَيَا صُحُفًا مُنَشَّرَةً"
  },
  {
    "id": 64,
    "start": 290.55,
    "end": 291.29,
    "text": "كَلَّا"
  },
  {
    "id": 65,
    "start": 291.97,
    "end": 295.04,
    "text": "أَلَّا يَخَافُونَ الْآخِرَةَ"
  },
  {
    "id": 66,
    "start": 296.02,
    "end": 300.15,
    "text": "كَلَّا إِنَّهُ تَذْكِرَةٌ"
  },
  {
    "id": 67,
    "start": 301.09,
    "end": 304.43,
    "text": "فَمَنْ شَاءَ ذَكَرَهُ"
  },
  {
    "id": 68,
    "start": 305.47,
    "end": 312.93,
    "text": "وَمَا يَذْكُرُونَ إِلَّا أَنْ يَشَاءَ اللَّهُ"
  },
  {
    "id": 69,
    "start": 313.87,
    "end": 317.84,
    "text": "وَأَهْلُ التَّقْوَى وَأَهْلُ الْمَغْفِرَةِ"
  }
]