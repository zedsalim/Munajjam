[
  {
    "id": 5496,
    "sura_id": 74,
    "index": 1,
    "text": "يَٰٓأَيُّهَا ٱلۡمُدَّثِّرُ",
    "start": 5.38,
    "end": 8.53
  },
  {
    "id": 5497,
    "sura_id": 74,
    "index": 2,
    "text": "قُمۡ فَأَنذِرۡ",
    "start": 9.46,
    "end": 11.07
  },
  {
    "id": 5498,
    "sura_id": 74,
    "index": 3,
    "text": "وَرَبَّكَ فَكَبِّرۡ",
    "start": 11.94,
    "end": 13.71
  },
  {
    "id": 5499,
    "sura_id": 74,
    "index": 4,
    "text": "وَثِيَابَكَ فَطَهِّرۡ",
    "start": 14.56,
    "end": 16.56
  },
  {
    "id": 5500,
    "sura_id": 74,
    "index": 5,
    "text": "وَٱلرُّجۡزَ فَٱهۡجُرۡ",
    "start": 17.51,
    "end": 19.13
  },
  {
    "id": 5501,
    "sura_id": 74,
    "index": 6,
    "text": "وَلَا تَمۡنُن تَسۡتَكۡثِرُ",
    "start": 20.08,
    "end": 22.51
  },
  {
    "id": 5502,
    "sura_id": 74,
    "index": 7,
    "text": "وَلِرَبِّكَ فَٱصۡبِرۡ",
    "start": 22.82,
    "end": 25.62
  },
  {
    "id": 5503,
    "sura_id": 74,
    "index": 8,
    "text": "فَإِذَا نُقِرَ فِي ٱلنَّاقُورِ",
    "start": 26.63,
    "end": 30.43
  },
  {
    "id": 5504,
    "sura_id": 74,
    "index": 9,
    "text": "فَذَٰلِكَ يَوۡمَئِذٖ يَوۡمٌ عَسِيرٌ",
    "start": 31.22,
    "end": 35.72
  },
  {
    "id": 5505,
    "sura_id": 74,
    "index": 10,
    "text": "عَلَى ٱلۡكَٰفِرِينَ غَيۡرُ يَسِيرٖ",
    "start": 36.54,
    "end": 40.36
  },
  {
    "id": 5506,
    "sura_id": 74,
    "index": 11,
    "text": "ذَرۡنِي وَمَنۡ خَلَقۡتُ وَحِيدٗا",
    "start": 41.41,
    "end": 44.67
  },
  {
    "id": 5507,
    "sura_id": 74,
    "index": 12,
    "text": "وَجَعَلۡتُ لَهُۥ مَالٗا مَّمۡدُودٗا",
    "start": 45.44,
    "end": 49.16
  },
  {
    "id": 5508,
    "sura_id": 74,
    "index": 13,
    "text": "وَبَنِينَ شُهُودٗا",
    "start": 49.97,
    "end": 52.0
  },
  {
    "id": 5509,
    "sura_id": 74,
    "index": 14,
    "text": "وَمَهَّدتُّ لَهُۥ تَمۡهِيدٗا",
    "start": 52.74,
    "end": 55.99
  },
  {
    "id": 5510,
    "sura_id": 74,
    "index": 15,
    "text": "ثُمَّ يَطۡمَعُ أَنۡ أَزِيدَ",
    "start": 56.79,
    "end": 60.23
  },
  {
    "id": 5511,
    "sura_id": 74,
    "index": 16,
    "text": "كـَلَّآۖ إِنَّهُۥ كَانَ لِأٓيَٰتِنَا عَنِيدٗا",
    "start": 61.01,
    "end": 67.02
  },
  {
    "id": 5512,
    "sura_id": 74,
    "index": 17,
    "text": "سَأُرۡهِقُهُۥ صَعُودًا",
    "start": 68.04,
    "end": 70.39
  },
  {
    "id": 5513,
    "sura_id": 74,
    "index": 18,
    "text": "إِنَّهُۥ فَكَّرَ وَقَدَّرَ",
    "start": 71.2,
    "end": 74.13
  },
  {
    "id": 5514,
    "sura_id": 74,
    "index": 19,
    "text": "فَقُتِلَ كَيۡفَ قَدَّرَ",
    "start": 74.9,
    "end": 76.99
  },
  {
    "id": 5515,
    "sura_id": 74,
    "index": 20,
    "text": "ثُمَّ قُتِلَ كَيۡفَ قَدَّرَ",
    "start": 77.84,
    "end": 80.79
  },
  {
    "id": 5516,
    "sura_id": 74,
    "index": 21,
    "text": "ثُمَّ نَظَرَ",
    "start": 81.68,
    "end": 83.14
  },
  {
    "id": 5517,
    "sura_id": 74,
    "index": 22,
    "text": "ثُمَّ عَبَسَ وَبَسَرَ",
    "start": 83.9,
    "end": 86.27
  },
  {
    "id": 5518,
    "sura_id": 74,
    "index": 23,
    "text": "ثُمَّ أَدۡبَرَ وَٱسۡتَكۡبَرَ",
    "start": 87.11,
    "end": 89.82
  },
  {
    "id": 5519,
    "sura_id": 74,
    "index": 24,
    "text": "فَقَالَ إِنۡ هَٰذَآ إِلَّا سِحۡرٞ يُؤۡثَرُ",
    "start": 90.86,
    "end": 96.47
  },
  {
    "id": 5520,
    "sura_id": 74,
    "index": 25,
    "text": "إِنۡ هَٰذَآ إِلَّا قَوۡلُ ٱلۡبَشَرِ",
    "start": 97.36,
    "end": 101.19
  },
  {
    "id": 5521,
    "sura_id": 74,
    "index": 26,
    "text": "سَأُصۡلِيهِ سَقَرَ",
    "start": 101.95,
    "end": 103.59
  },
  {
    "id": 5522,
    "sura_id": 74,
    "index": 27,
    "text": "وَمَآ أَدۡرَىٰكَ مَا سَقَرُ",
    "start": 104.54,
    "end": 107.54
  },
  {
    "id": 5523,
    "sura_id": 74,
    "index": 28,
    "text": "لَا تُبۡقِي وَلَا تَذَرُ",
    "start": 108.32,
    "end": 110.74
  },
  {
    "id": 5524,
    "sura_id": 74,
    "index": 29,
    "text": "لَوَّاحَةٞ لِّلۡبَشَرِ",
    "start": 111.43,
    "end": 113.7
  },
  {
    "id": 5525,
    "sura_id": 74,
    "index": 30,
    "text": "عَلَيۡهَا تِسۡعَةَ عَشَرَ",
    "start": 114.38,
    "end": 116.63
  }
]